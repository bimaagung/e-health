{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-await-in-loop": "off",
        "no-continue":"off",
        "prefer-const":"off",
        "consistent-return":"off",
        "linebreak-style":"off",
        "no-return-await":"off",
        "no-param-reassign":"off",
        "no-underscore-dangle":"off",
        "class-methods-use-this":"off",
        "no-useless-constructor":"off",
        "no-empty-function":"off",
        "max-len":"off"
    }
}
